  <managed-bean>
    <description>
      Helper för status.jsp
    </description>
    <managed-bean-name>statusBean</managed-bean-name>
    <managed-bean-class>se.csn.notmotor.admin.beans.StatusBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <description>
      Helper för params.jsp
    </description>
    <managed-bean-name>paramBean</managed-bean-name>
    <managed-bean-class>se.csn.notmotor.admin.beans.ParamBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <description>
      Helper för statistik.jsp
    </description>
    <managed-bean-name>statBean</managed-bean-name>
    <managed-bean-class>se.csn.notmotor.admin.beans.StatistikBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <description>
      Helper för sok.jsp
    </description>
    <managed-bean-name>sokBean</managed-bean-name>
    <managed-bean-class>se.csn.notmotor.admin.beans.SokBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <description>
      Helper för stangningsschema.jsp
    </description>
    <managed-bean-name>schemaBean</managed-bean-name>
    <managed-bean-class>se.csn.notmotor.admin.beans.SchemaBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <description>
      Helper för visameddelande.jsp
    </description>
    <managed-bean-name>msgBean</managed-bean-name>
    <managed-bean-class>se.csn.notmotor.admin.beans.MeddelandeBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <lifecycle>
  	<phase-listener>com.ibm.faces.webapp.ValueResourcePhaseListener</phase-listener>
  </lifecycle>
